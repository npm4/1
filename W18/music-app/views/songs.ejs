<!DOCTYPE html>
<html>
<head>
    <title>Music App</title>
    <style>
        table { width: 80%; margin: 20px auto; border-collapse: collapse; }
        th, td { padding: 10px; border: 1px solid #999; text-align: center; }
    </style>
</head>
<body>
    <h2 style="text-align:center;">Total Songs: <%= count %></h2>
    <table>
        <tr>
            <th>Song Name</th>
            <th>Film Name</th>
            <th>Music Director</th>
            <th>Singer</th>
            <th>Actor</th>
            <th>Actress</th>
        </tr>
        <% songs.forEach(song => { %>
            <tr>
                <td><%= song.Songname %></td>
                <td><%= song.Film %></td>
                <td><%= song.Music_director %></td>
                <td><%= song.Singer %></td>
                <td><%= song.Actor || '---' %></td>
                <td><%= song.Actress || '---' %></td>
            </tr>
        <% }) %>
    </table>
</body>
</html>
